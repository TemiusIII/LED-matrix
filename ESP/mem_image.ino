void rainbow(int wait = 10, int BRIGHTNESS = 255 , int SATURATION = 255) {
  for (int j = 0; j < 255; j++) {
    for (int i = 0; i < NUM_LEDS; i++) {
      leds[i] = CHSV(i - (j * 2), SATURATION, BRIGHTNESS);
    }
    FastLED.show();
    delay(wait);
  }
}
